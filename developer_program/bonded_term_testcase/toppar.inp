BOMLEV -3

set toppar ../../toppar/c36

read rtf card name              @toppar/top_all36_prot.rtf
read param card flex name       @toppar/par_all36_prot.prm
read rtf card append name       @toppar/top_all36_cgenff.rtf
read para card flex append name @toppar/par_all36_cgenff.prm
read rtf card append name       @toppar/top_all36_na.rtf
read para card flex append name @toppar/par_all36_na.prm
read rtf card append name       @toppar/top_all36_carb.rtf
read para card flex append name @toppar/par_all36_carb.prm
stream                          @toppar/toppar_water_ions.str

BOMLEV -5

! topology with CGenFF v5.0 charges
read rtf card append name top.inp

! optimized parameters in other molecules
read para card flex append name param_new.prm 
! parameters generated by developer
read para card flex append name add_param.prm 
